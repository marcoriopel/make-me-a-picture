{
  "name": "client",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "start:local": "ng serve --open",
    "start:prod": "ng serve --prod --open",
    "start:electron": "ng build --base-href ./ && electron .",
    "format": "prettier --config ../.prettierrc.yaml --write \"src/**/*\"",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "lint": "ng lint --type-check --format stylish",
    "lint:fix": "npm run lint -- --fix",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/LogoDark.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . \"Fais moi un dessin\" --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/LogoDark.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Guess & Draw\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/LogoDark.png --prune=true --out=release-builds",
    "build:electron": "ng build --prod --base-href ./ && electron-packager . \"faisMoiUnDessin\" --asar=true --ignore=/node_modules --icon=src/assets/icons/win/LogoDark.ico --ignore=/e2e --ignore=/src --overwrite --out=./build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.9",
    "@angular/cdk": "^10.2.1",
    "@angular/common": "~10.0.9",
    "@angular/compiler": "~10.0.9",
    "@angular/core": "~10.0.9",
    "@angular/forms": "~10.0.9",
    "@angular/material": "^10.2.1",
    "@angular/platform-browser": "~10.0.9",
    "@angular/platform-browser-dynamic": "~10.0.9",
    "@angular/router": "~10.0.9",
    "@material/textfield": "^10.0.0",
    "electron-packager": "^15.2.0",
    "material-design-icons": "^3.0.1",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.5",
    "socket.io-client": "^3.1.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "~10.0.9",
    "@angular/language-service": "~10.0.9",
    "@types/canvas-confetti": "^1.4.0",
    "@types/crypto-js": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "canvas-confetti": "^1.4.0",
    "codelyzer": "^6.0.0",
    "crypto-js": "^4.0.0",
    "electron": "^11.2.1",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jsonwebtoken": "^8.5.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  }
}
